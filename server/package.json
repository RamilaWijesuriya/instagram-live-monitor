{
  "name": "instagram-live-server",
  "version": "1.0.0",
  "description": "Express server for Instagram Live Monitor",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch src --ext ts,json --exec \"node --loader ts-node/esm src/index.ts\"",
    "start": "node dist/index.js",
    "lint": "ts-node scripts/run-and-log.ts npx eslint . --cache --format=json",
    "lint:retry": "ts-node scripts/rerun-errors.ts",
    "test": "jest",
    "test:log": "ts-node scripts/run-and-log.ts npm test",
    "test:retry": "ts-node scripts/rerun-errors.ts"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "instagram-private-api": "*",
    "mongoose": "^6.8.0",
    "p-limit": "^4.0.0",
    "socket.io": "^4.6.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.9",
    "@types/socket.io": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.1.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.4",
    "typescript": "^4.8.4",
    "typescript-eslint": "^8.32.1"
  }
}
